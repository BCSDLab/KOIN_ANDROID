                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center_horizontal"
                android:lineSpacingExtra="5sp"
                android:text="BCSD Lab "
                android:textColor="@color/black"
                android:textSize="12sp"
