            app:layout_constraintBottom_toTopOf="@id/guideline_horizontal_0.353"
            app:layout_constraintEnd_toStartOf="@+id/guideline_vertical_0.889"
            app:layout_constraintStart_toStartOf="@+id/guideline_vertical_0.122"
            app:layout_constraintTop_toBottomOf="@+id/guideline_horizontal_0.319">

