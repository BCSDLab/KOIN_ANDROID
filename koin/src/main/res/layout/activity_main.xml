                                android:background="@drawable/button_border_bottom_left_white_two_1dp"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:orientation="vertical">

                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:id="@+id/activity_main_circle_constraint_layout"
                                    android:layout_margin="1dp"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:background="@drawable/bg_main_button"
                                    android:gravity="center"
                                    android:orientation="vertical">

                                    <androidx.constraintlayout.widget.Guideline
                                        android:id="@+id/guideline_circle_vertical_0.244"
                                        android:layout_width="1dp"
                                        android:layout_height="wrap_content"
                                        android:orientation="horizontal"
                                        app:layout_constraintGuide_percent="0.244" />

                                    <androidx.constraintlayout.widget.Guideline
                                        android:id="@+id/guideline_circle_vertical_0.58"
                                        android:layout_width="1dp"
                                        android:layout_height="wrap_content"
                                        android:orientation="horizontal"
                                        app:layout_constraintGuide_percent="0.58" />

                                    <ImageView
                                        android:id="@+id/circle_imageview"
                                        android:layout_width="24dp"
                                        android:layout_height="24dp"
                                        android:background="@drawable/ic_circles"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="@+id/guideline_circle_vertical_0.244" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        app:font_name="Custom5"
                                        android:text="동아리"
                                        android:textColor="@color/warm_grey_three"
                                        android:textSize="15sp"
                                        android:textStyle="bold"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="@+id/guideline_circle_vertical_0.58" />


                                </androidx.constraintlayout.widget.ConstraintLayout>
                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/activity_main_bus_linear_layout"
                                android:background="@drawable/button_border_bottom_left_white_two_1dp"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:orientation="vertical">

                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:id="@+id/activity_main_bus_constraint_layout"
                                    android:background="@drawable/bg_main_button"
                                    android:layout_margin="1dp"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">

                                    <androidx.constraintlayout.widget.Guideline
                                        android:id="@+id/guideline_bus_vertical_0.244"
                                        android:layout_width="1dp"
                                        android:layout_height="wrap_content"
                                        android:orientation="horizontal"
                                        app:layout_constraintGuide_percent="0.244" />

                                    <androidx.constraintlayout.widget.Guideline
                                        android:id="@+id/guideline_bus_vertical_0.58"
                                        android:layout_width="1dp"
                                        android:layout_height="wrap_content"
                                        android:orientation="horizontal"
                                        app:layout_constraintGuide_percent="0.58" />

                                    <ImageView
                                        android:id="@+id/imageView2"
                                        android:layout_width="24dp"
                                        android:layout_height="24dp"
                                        android:background="@drawable/ic_bus"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="@+id/guideline_bus_vertical_0.244" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        app:font_name="Custom5"
                                        android:text="버스/교통"
                                        android:textColor="@color/warm_grey_three"
                                        android:textSize="15sp"
                                        android:textStyle="bold"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="@+id/guideline_bus_vertical_0.58" />

                                </androidx.constraintlayout.widget.ConstraintLayout>

                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/activity_main_timetable_linear_layout"
                                android:background="@drawable/button_border_bottom_left_white_two_1dp"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:orientation="vertical">

                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:id="@+id/activity_main_timetable_constraint_layout"
                                    android:layout_margin="1dp"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:background="@drawable/bg_main_button"
                                    android:gravity="center"
                                    android:orientation="vertical">

                                    <androidx.constraintlayout.widget.Guideline
                                        android:id="@+id/guideline_timetable_vertical_0.244"
                                        android:layout_width="1dp"
                                        android:layout_height="wrap_content"
                                        android:orientation="horizontal"
                                        app:layout_constraintGuide_percent="0.244" />

                                    <androidx.constraintlayout.widget.Guideline
                                        android:id="@+id/guideline_timetable_vertical_0.58"
                                        android:layout_width="1dp"
                                        android:layout_height="wrap_content"
                                        android:orientation="horizontal"
                                        app:layout_constraintGuide_percent="0.58" />

                                    <ImageView
                                        android:id="@+id/timetable_imageview"
                                        android:layout_width="24dp"
                                        android:layout_height="24dp"
                                        android:background="@drawable/ic_timetable"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="@+id/guideline_timetable_vertical_0.244" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        app:font_name="Custom5"
                                        android:text="시간표"
                                        android:textColor="@color/warm_grey_three"
                                        android:textSize="15sp"
                                        android:textStyle="bold"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="@+id/guideline_timetable_vertical_0.58" />


                                </androidx.constraintlayout.widget.ConstraintLayout>
                            </LinearLayout>

                        </TableRow>

                        <TableRow
                            android:layout_height="0dp"
                            android:layout_weight="1">

                            <LinearLayout
