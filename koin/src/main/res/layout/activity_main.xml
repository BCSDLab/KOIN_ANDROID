                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="@+id/guideline_horizental_0.083"
                    app:layout_constraintTop_toTopOf="@id/guideline_vertical_0.173" />

