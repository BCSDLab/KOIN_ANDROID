                                        app:layout_constraintTop_toTopOf="@+id/guideline_dining_vertical_0.244" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        app:font_name="Custom5"
                                        android:text="식단"
                                        android:textColor="@color/warm_grey_three"
                                        android:textSize="15sp"
                                        android:textStyle="bold"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="@+id/guideline_dining_vertical_0.58" />

                                </androidx.constraintlayout.widget.ConstraintLayout>
                            </LinearLayout>


                            <LinearLayout
                                android:id="@+id/activity_main_used_market_linear_layout"
                                android:background="@drawable/button_border_bottom_left_white_two_1dp"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:orientation="vertical">

                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:id="@+id/activity_main_used_market_constraint_layout"
                                    android:background="@drawable/bg_main_button"
